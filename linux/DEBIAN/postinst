mkdir /usr/share/DuckSploit/
mkdir /usr/share/DuckSploit/icons

version=1.0.8

				     
chmod +x ds
chmod +x server.py

sudo cp -r ds /usr/local/sbin/
sudo cp DuckSploit.desktop /usr/share/applications/
cp -r server.py /usr/share/DuckSploit/
cp -r icon.png /usr/share/DuckSploit/icons/

clear
	
echo "\e[0;32m[o] DUCKSLOIT $version is now installed on your machine!"
